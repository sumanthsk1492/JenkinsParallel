pipeline {
  agent any
  stages {
    stage ('BUILD') {
      steps {
        echo 'This is Build Stage'
        sh '''
              sleep 5
              exit 0
            '''
      }
    }
    stage ('TEST PARALLEL') {
        parallel {
          stage ('TEST ON CHROME') {
            steps {
              echo 'This is test on Chrome Browser'
              sh 'sleep 5; exit 1'
            }
          }
         stage ('TEST ON EXPLORER') {
            steps {
              echo 'This is test on Explorer'
              sh 'sleep 5; exit 1'
            }
          }
        }
    }
